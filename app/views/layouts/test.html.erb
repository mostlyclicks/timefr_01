<style>
#map_us { -webkit-transform: translateZ(0) !important; }
</style>

<script type="text/javascript">


function initMap() {
  var myLatLng = {lat: -25.363, lng: 131.044};

  var map = new google.maps.Map(document.getElementById('map_us'), {
    zoom: 4,
    center: myLatLng
  });

  var marker = new google.maps.Marker({
    position: myLatLng,
    map: map,
    title: 'Hello World!'
  });
}
</script>

<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD_8tnzed9azG-izQV6zqi96yPsQDoV_qc&callback=initMap"></script>
